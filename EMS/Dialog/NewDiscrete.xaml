<Window x:Class="EMS.Dialog.NewDiscrete"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EMS.Dialog"
        mc:Ignorable="d"
        Title="NewDiscrete" Height="255" Width="300">
    <Grid>
        <StackPanel>
            <TextBlock Text="Node name:" />
            <Label />
            <TextBox x:Name="tb_nodeName" Width="250" HorizontalAlignment="Left"/>
            <Label/>
            <TextBlock Text="Value = " />
            <Label />
            <TextBox x:Name="tb_value" Width="250" HorizontalAlignment="Left" />
            <Label />
            <TextBlock Text="Added Values: " x:Name="tb_Output" />
            <Label />
            <Button Content="Add Value" Click="AddButton_Click" />
            <Label />
            <Button Content="Print Values" Click="PrintButton_Click" />
            <Label />
            <Button Content="OK" Click="OKButton_Click" />
        </StackPanel>
    </Grid>
</Window>
