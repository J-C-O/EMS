<Window x:Class="EMS.Dialog.NewSubGraphDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EMS.Dialog"
        mc:Ignorable="d"
        Title="NewNodeDialog" Height="200" Width="300">
    <Grid>
        <StackPanel>
            <TextBlock Text="Root name:" />
            <Label />
            <TextBox x:Name="tb_rootName" Width="250" HorizontalAlignment="Left"/>
            <Label/>
            <TextBlock Text="Child name:" />
            <Label />
            <TextBox x:Name="tb_childName" Width="250" HorizontalAlignment="Left"/>
            <Label/>
            <Button Content="OK" Click="OKButton_Click" />
        </StackPanel>
    </Grid>
</Window>
