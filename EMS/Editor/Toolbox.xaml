<UserControl x:Class="EMS.Editor.Toolbox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EMS.Editor"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>
        <DockPanel x:Name="PanelParallel" Grid.Column="0" Grid.Row="0" Background="#ffcc99" />

        <DockPanel x:Name="PanelAlternative" Grid.Column="0" Grid.Row="1" Background="#fff2e6" />

        <DockPanel x:Name="PanelDiscrete" Grid.Column="0" Grid.Row="2" Background="#ffcc99" />

        <DockPanel x:Name="PanelContinuous" Grid.Column="0" Grid.Row="3" Background="#fff2e6" />

    </Grid>
</UserControl>
